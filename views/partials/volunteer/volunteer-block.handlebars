<div class="pure-u-1 pure-u-md-1-3">
    <div class="pricing-table pricing-table-free">
        <div class="pricing-table-header">
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcS9yRhYhEq3Z2tm854A4-w6E8TCjcNwg-jt_VTKF0PS5SZdw-dG&usqp=CAU"
                width="100%" height="100%">
            <span class="pricing-table-price">
            </span>
        </div>

        <ul class="pricing-table-list">
            <li>{{eventTitle}}</li>
            <li>{{eventStartDate}}</li>
            <li>{{eventEndDate}}</li>
            <li>{{regLimit}}</li>
            <li>{{effort}}</li>
            <li>{{eventDetail}}</li>
            <li>{{eventId}}</li>
            <li>{{address}}</li>
            <li>{{eventStartTime}}</li>
            <li>{{eventEndTime}}</li>
            <li>{{duration}}</li>
            <li>{{eventSummary}}</li>
            <li>{{createdAt}}</li>


        </ul>

        <button class="button-choose pure-button">Choose</button>
    </div>
</div>