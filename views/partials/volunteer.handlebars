 <link rel="stylesheet" href="/public/css/volunteer.css">

<div class="pure-u-1 pure-u-md-1-3">
    <div class="pricing-table pricing-table-free">
        <div class="pricing-table-header">
            <img src="/public/img/Volunteer.jfif"
                width="100%" height="100%">
            <span class="pricing-table-price">
            </span>
        </div>

        <ul class="pricing-table-list">
            <li>{{eventTitle}}</li>
            <li>{{eventStartDate}}</li>
            <li>{{eventEndDate}}</li>
            <li>{{regLimit}}</li>
            <li>{{effort}}</li>
            <li>{{eventDetail}}</li>
            <li>{{eventId}}</li>
            <li>{{address}}</li>
            <li>{{eventStartTime}}</li>
            <li>{{eventEndTime}}</li>
            <li>{{duration}}</li>
            <li>{{eventSummary}}</li>
            <li>{{createdAt}}</li>


        </ul>

        <button class="button-choose pure-button">Choose</button>
    </div>
</div>